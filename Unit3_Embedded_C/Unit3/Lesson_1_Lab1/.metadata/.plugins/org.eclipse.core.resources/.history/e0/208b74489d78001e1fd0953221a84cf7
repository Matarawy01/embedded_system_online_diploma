#include "Platform_Types.h"

/* Register Address */
#define RCC_Base_Address    0x40021000
#define GPIO_PortA          0x40010800
#define APB2ENR             *((volatile unit32_t *) RCC_Base_Address + 0x18)
#define CRH                 *((volatile unit32_t *) GPIO_PortA + 0x04)
#define ODR                 *((volatile unit32_t *) GPIO_PortA + 0x0c)

int main(void)
{
	int i;
    APB2ENR |= (1<<2);
    ODR |=(1<<13);
    CRH &=0xFF0FFFFF;
    CRH |=0x00200000;
    while(1)
    {
    	ODR |=(1<<13);
    	for(i = 0; i < 5000; i++);
    	ODR &=~(1<<13);
    	for(i = 0; i < 5000; i++);
    }
}
